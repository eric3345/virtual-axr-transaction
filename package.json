{
  "name": "virtual-axr-transaction",
  "version": "1.0.0",
  "description": "OpenClaw skill to send multiple swap_token transactions with Axelrod agent. Supports configurable transaction count, custom swap parameters, and agent info query.",
  "main": "dist/src/lib/axelrod-monitor.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "check_status": "npx tsx scripts/index.ts check_status",
    "transaction": "npx tsx scripts/index.ts transaction",
    "test": "npx tsx scripts/test.ts",
    "test-skill": "echo 'Usage: LITE_AGENT_API_KEY=your-key npm run test-skill'"
  },
  "bin": {
    "axr-monitor": "./dist/scripts/index.js"
  },
  "keywords": [
    "openclaw",
    "skill",
    "virtuals-protocol",
    "axelrod",
    "agent-monitoring",
    "graduation-status",
    "lite-agent-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}